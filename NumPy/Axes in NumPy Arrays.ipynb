   "source": [
    "#Axes in NumPy Arrays\n",
    "import numpy as np\n",
    "arr = np.array([[1, 2], [3, 4], [5, 6]])\n",
    "print(\"Original array:\\n\", arr)\n",
    "\n",
    "print(\"sum of columns:\", np.sum(arr, axis=0))  # Output: [9 12] (Sum of columns)\n",
    "print(\"sum of rows:\", np.sum(arr, axis=1))  # Output: [3 7 11] (Sum of rows)"
   ]
  }
