    "\n",
    "#Reshape a NumPy Array\n",
    "import numpy as np\n",
    "arr = np.array([1, 2, 3, 4, 5, 6])\n",
    "reshaped_arr_2d = arr.reshape(2, 3)  # Reshaping to 2x3 matrix\n",
    "reshaped_arr_3d = arr.reshape(2, 1, 3)  # Reshaping to 2x1x3 matrix\n",
    "print(reshaped_arr_2d)\n",
    "print(reshaped_arr_3d)\n"
   ]
  }
