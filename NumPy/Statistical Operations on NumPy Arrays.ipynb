    "#Statistical Operations on NumPy Arrays\n",
    "import numpy as np\n",
    "arr = np.array([1, 2, 3, 4, 5])\n",
    "mean_val = np.mean(arr)\n",
    "std_val = np.std(arr)\n",
    "print(\"Mean:\", mean_val)  \n",
    "print(\"Standard Deviation:\", std_val)"
